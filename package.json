{
  "name": "worqhat",
  "version": "2.0.0",
  "description": "NodeJS SDK for WorqHat Endpoints",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "del-cli ./lib",
    "build": "npx prettier --write . && npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && move-file lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "mocha",
    "prettier": "npx prettier --write .",
    "semantic-release": "semantic-release",
    "prepack": "npm run build",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WorqHat/worqhat-node.git"
  },
  "keywords": [
    "nodejs",
    "sdk",
    "worqhat",
    "worqhat-sdk",
    "worqhat-node"
  ],
  "author": "WorqHat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WorqHat/worqhat-node/issues"
  },
  "homepage": "https://docs.worqhat.com/node-js-sdk/initialization",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "del-cli": "^5.0.0",
    "mocha": "^10.0.0",
    "move-file-cli": "^3.0.0",
    "semantic-release": "^21.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "form-data": "^4.0.0",
    "sharp": "^0.32.6"
  }
}
